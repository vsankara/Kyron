               <xsd:element name="TaxType" type="TaxTypeDetails" minOccurs="0"
                            maxOccurs="unbounded"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="RegisterTaxTypeRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType"/>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:complexType name="TaxTypeDetails">
          <xsd:sequence>
               <xsd:element name="DetailType" type="xsd:string"/>
               <xsd:element name="ReturnType" type="xsd:string" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element name="SubmissionDate" type="xsd:date"
                            minOccurs="0"/>
               <xsd:element name="EDR" type="xsd:date"/>
               <xsd:element name="TaxableTurnover" type="xsd:decimal"
                            minOccurs="0"/>
               <!-- Element OverrideFilingFrequency Not used , commented the code as per TRGHA-7350 for PAYE Tax Type-->
               <!--
			<xsd:element name="OverrideFilingFrequency" type="xsd:string" minOccurs="0"/>
			<xsd:element name="OverrideStartMonth" type="bt:MonthType" minOccurs="0"/>
			-->
               <xsd:element name="AccountStatus" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="ForcedRegistration" type="xsd:boolean"
                            minOccurs="0"/>
          </xsd:sequence>
     </xsd:complexType>
     <!-- *********************************** End  Register TaxType Types *********************************** -->
     <!-- *********************************** Start Suspend TaxType Types *********************************** -->
     <xsd:complexType name="SuspendTaxType">
          <xsd:sequence>
               <xsd:element name="TIN" type="xsd:string"/>
               <xsd:element name="DetailType" type="xsd:string"/>
               <xsd:element name="EDR" type="xsd:date"/>
               <xsd:element name="AccountStatus" type="xsd:string"/>
               <xsd:element name="SuspensionStartDate" type="xsd:date"/>
               <xsd:element name="SuspensionEndDate" type="xsd:date"
                            minOccurs="0"/>
               <xsd:element name="Reason" type="xsd:string"/>
               <xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="SuspendTaxTypeRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType"/>
          </xsd:complexContent>
     </xsd:complexType>
     <!-- *********************************** End Suspend TaxType Types *********************************** -->
     <xsd:complexType name="DeregisterTaxType">
          <xsd:sequence>
               <xsd:element name="TIN" type="xsd:string"/>
               <xsd:element name="TaxType" type="xsd:string"/>
               <xsd:element name="EDD" type="xsd:date"/>
               <xsd:element name="DeRegistrationReason" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="DeregisterTaxTypeRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType"/>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:complexType name="ReactivateTaxType">
          <xsd:sequence>
               <xsd:element name="TIN" type="xsd:string"/>
               <xsd:element name="TaxType" type="xsd:string"/>
               <xsd:element name="ReactivationDate" type="xsd:date"/>
               <xsd:element name="Reason" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="ReactivateTaxTypeRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType"/>
          </xsd:complexContent>
     </xsd:complexType>
     <!-- *********************************** Start Transfer Taxpayer Types *********************************** -->
     <xsd:complexType name="TransferTaxpayer">
          <xsd:sequence>
               <xsd:element name="TIN" type="xsd:string"/>
               <xsd:element name="CurrentOffice" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="NewOffice" type="xsd:string"/>
               <xsd:element name="DateofTransfer" type="xsd:date"/>
               <xsd:element name="Reason" type="xsd:string"/>
               <xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="TransferTaxpayerRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType"/>
          </xsd:complexContent>
     </xsd:complexType>
     <!-- *********************************** End Transfer Taxpayer Types *********************************** -->
     <!-- *************************************** Start Payment Types *************************************** -->
     <xsd:complexType name="GetPaymentReceipt">
          <xsd:sequence>
               <xsd:element name="ReceiptNo" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="GetPaymentReceiptRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType">
                    <xsd:sequence>
                         <xsd:element name="PaymentDetails"
                                      type="PaymentDetailsType" minOccurs="0"/>
                    </xsd:sequence>
               </xsd:extension>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:complexType name="PaymentDetailsType">
          <xsd:complexContent>
               <xsd:extension base="FileAttachmentType"/>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:complexType name="FileAttachmentType">
          <xsd:sequence>
               <xsd:element name="FileName" type="xsd:string"/>
               <xsd:element name="FileType" type="xsd:string" minOccurs="0"/>
               <xsd:element name="FileAttachment" type="xsd:base64Binary"
                            xmime:expectedContentTypes="application/octet-stream"/>
          </xsd:sequence>
     </xsd:complexType>
     <!-- *************************************** End Payment Types *************************************** -->
     <!-- ************************************ Start Transaction Types ************************************ -->
     <xsd:complexType name="ExtractInvoiceTransactionsReq">
          <xsd:sequence>
               <xsd:element name="DateTimeFrom" type="xsd:dateTime"/>
               <xsd:element name="DateTimeTo" type="xsd:dateTime"/>
               <xsd:element name="CalledBy" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="ExtractInvoiceTransactionsRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType">
                    <xsd:sequence>
                         <xsd:element name="Transactions"
                                      type="TransExtInvoiceType" minOccurs="0"
                                      maxOccurs="unbounded"/>
                    </xsd:sequence>
               </xsd:extension>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:complexType name="ExtractPaymentTransactionsReq">
          <xsd:sequence>
               <xsd:element name="DateTimeFrom" type="xsd:dateTime"/>
               <xsd:element name="DateTimeTo" type="xsd:dateTime"/>
               <xsd:element name="CalledBy" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="ExtractPaymentTransactionsRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType">
                    <xsd:sequence>
                         <xsd:element name="Transactions"
                                      type="TransExtPaymentType" minOccurs="0"
                                      maxOccurs="unbounded"/>
                    </xsd:sequence>
               </xsd:extension>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:element name="TransExtInvoiceType" type="TransExtInvoiceType"/>
     <xsd:complexType name="TransExtInvoiceType">
          <xsd:sequence>
               <!-- CREDIT or DEBIT -->
               <xsd:element name="Type" type="xsd:string"/>
               <!-- Posted transaction amount -->
               <xsd:element name="Amount" type="xsd:decimal"/>
               <!-- The generated transaction reference number -->
               <xsd:element name="Number" type="xsd:string"/>
               <!-- Encoded period number: num/year -->
               <xsd:element name="PeriodNumber" type="xsd:string"
                            minOccurs="0"/>
               <!-- The taxpayer account number the transaction belongs to -->
               <xsd:element name="AccountNumber" type="xsd:string"/>
               <!-- Date and time that the transaction was recorded -->
               <xsd:element name="Date" type="xsd:dateTime"/>
               <!-- Revenue Ledger code generated from the trips posting rules -->
               <xsd:element name="RevLedCode" type="xsd:string"/>
               <!-- Optional description of the posted transaction -->
               <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
               <!-- Office code from REF_OFFICE -->
               <xsd:element name="Office" type="xsd:string" minOccurs="0"/>
               <!-- Document type -->
               <xsd:element name="DocumentType" type="xsd:string"/>
               <!-- The taxpayer TIN the transaction belongs to -->
               <xsd:element name="TIN" type="xsd:string"/>
               <!-- Document Event type -->
               <xsd:element name="DocumentEventType" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="TaxType" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:element name="TransExtPaymentType" type="TransExtPaymentType"/>
     <xsd:complexType name="TransExtPaymentType">
          <xsd:sequence>
               <!-- Currency that the payment was received in -->
               <xsd:element name="Currency" type="xsd:string"/>
               <!-- Amount Received -->
               <xsd:element name="PaymentAmount" type="xsd:decimal"/>
               <xsd:element name="PaymentDate" type="xsd:dateTime"/>
               <!-- Location where the payment was received -->
               <xsd:element name="Till" type="xsd:string"/>
               <xsd:element name="ExchangeRate" type="xsd:decimal"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="ExtractTransactionsReq">
          <xsd:sequence>
               <xsd:element name="DateTimeFrom" type="xsd:dateTime"/>
               <xsd:element name="DateTimeTo" type="xsd:dateTime"/>
               <xsd:element name="CalledBy" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="ExtractTransactionsRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType">
                    <xsd:sequence>
                         <xsd:element name="Transactions" type="TransExtType"
                                      minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
               </xsd:extension>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:element name="TransExtType" type="TransExtType"/>
     <xsd:complexType name="TransExtType">
          <xsd:sequence>
               <!-- CREDIT or DEBIT -->
               <xsd:element name="TransType" type="xsd:string"/>
               <!-- Posted transaction amount -->
               <xsd:element name="Amount" type="xsd:decimal"/>
               <!-- Document type -->
               <xsd:element name="DocType" type="xsd:string"/>
               <!-- Document type -->
               <xsd:element name="DocEventType" type="xsd:string"
                            minOccurs="0"/>
               <!-- The generated transaction reference number -->
               <xsd:element name="TransReference" type="xsd:string"/>
               <!-- The taxpayer TIN the transaction belongs to -->
               <xsd:element name="TIN" type="xsd:string"/>
               <!-- The taxpayer account number the transaction belongs to -->
               <xsd:element name="AccountNumber" type="xsd:string"/>
               <!-- Date and time that the transaction was recorded -->
               <xsd:element name="TransDate" type="xsd:dateTime"/>
               <!-- Revenue Ledger code generated from the trips posting rules -->
               <xsd:element name="RevLedCode" type="xsd:string"/>
               <xsd:element name="TaxType" type="xsd:string"/>
               <!-- Optional description of the posted transaction -->
               <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
               <!-- Office code from REF_OFFICE -->
               <xsd:element name="Office" type="xsd:string" minOccurs="0"/>
               <xsd:element name="ReceiptNumber" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="AllocationItemType" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="PaymentAllocationTaxOffice" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="PaymentCurrency" type="xsd:string"
                            minOccurs="0"/>
               <xsd:element name="AmountInForex" type="xsd:decimal"
                            minOccurs="0"/>
               <xsd:element name="ExchangeRate" type="xsd:decimal"
                            minOccurs="0"/>
          </xsd:sequence>
     </xsd:complexType>
     <!-- ************************************* End Transaction Types ************************************* -->
     <!-- ************************************ Start View Available Return Types ************************************ -->
     <xsd:complexType name="ViewAvailableReturnDetailsReq">
          <xsd:sequence>
               <xsd:element name="TIN" type="xsd:string"/>
          </xsd:sequence>
     </xsd:complexType>
     <xsd:complexType name="ViewAvailableReturnDetailsRes">
          <xsd:complexContent>
               <xsd:extension base="ServiceResponseType">
                    <xsd:sequence>
                         <xsd:element name="TIN" type="xsd:string"
                                      minOccurs="0"/>
                         <xsd:element name="ReturnDocumentDetails"
                                      type="ReturnDocumentDetailsType"
                                      minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
               </xsd:extension>
          </xsd:complexContent>
     </xsd:complexType>
     <xsd:element name="ReturnDocumentDetailsType"
                  type="ReturnDocumentDetailsType"/>
     <xsd:complexType name="ReturnDocumentDetailsType">
          <xsd:sequence>
               <xsd:element name="DocumentType" type="xsd:string"/>
               <xsd:element name="PeriodYear" type="xsd:string"/>
               <xsd:element name="PeriodNumber" type="xsd:string"/>
               <xsd:element name="PeriodFromDate" type="xsd:date"
                            minOccurs="0"/>
          </xsd:sequence>
     </xsd:complexType>
     <!-- ************************************* End View Return Document Types ************************************ -->
</xsd:schema>