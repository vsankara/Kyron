 <searchList>
  <!--SEARCH HOLIDAY-->
  <search>
    <note>USED FOR HOLIDAY SEARCH</note>
    <id>FIND_DATE_HOLIDAY</id>
    <sectionHeading>Find Holidays</sectionHeading>
    <windowHeading>TRIPS - Search Holidays</windowHeading>
    <showCreateNewButton>Y</showCreateNewButton>
    <criteriaFields>
      <field>
        <fieldName>Year</fieldName>
        <fieldType>number</fieldType>
        <fieldLabel>Year</fieldLabel>
        <columnName>REF_DATE_CFG.YEAR</columnName>
      </field>
    </criteriaFields>
    <resultFields>
      <field>
        <columnOrderNbr>1</columnOrderNbr>
        <fieldName>Code</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>CODE</columnHeading>
        <columnName>REF_DATE_CFG.CODE</columnName>
        <isVisible>Y</isVisible>
      </field>
      <field>
        <columnOrderNbr>2</columnOrderNbr>
        <fieldName>Description</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>DESCRIPTION</columnHeading>
        <columnName>REF_DATE_CFG.DESCRIPTION</columnName>
      </field> 
      <field>
        <columnOrderNbr>3</columnOrderNbr>
        <fieldName>Date</fieldName>
        <fieldType>date</fieldType>
        <columnHeading>DATE</columnHeading>
        <columnName>REF_DATE_CFG.DATE_FROM</columnName>
      </field>
      <field>
        <columnOrderNbr>4</columnOrderNbr>
        <fieldName>Status</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>STATUS</columnHeading>
        <columnName>REF_DATE_CFG.STATUS</columnName>
      </field>
    </resultFields>
    <searchTables>
        <table>
            <name>REF_DATE_CFG</name>
        </table>
    </searchTables>
    <joinQuery>REF_DATE_CFG.REF_DATE_CATEGORY='HD'</joinQuery>
    <where>REF_DATE_CFG.REF_DATE_CATEGORY like 'HD'</where>
    <actions>
      <action>  
        <actionId>view</actionId>
        <actionLabel>View</actionLabel>
        <actionCallBack>view</actionCallBack>
        <allowableModes>viewMode</allowableModes>
      </action>
      <action>
        <actionId>edit</actionId>
        <actionLabel>Update</actionLabel>
        <actionCallBack>edit</actionCallBack>
        <allowableModes>editMode</allowableModes>
      </action>
      <action>
        <actionId>add</actionId>
        <actionLabel>Add</actionLabel>
        <actionCallBack>add</actionCallBack>
        <allowableModes>addMode</allowableModes>
      </action>
    </actions>
  </search>
  <!--SEARCH LEGISLATIVE DATE--> 
  <search>
    <primaryTableName>REF_DATE_CFG</primaryTableName>
    <note>USED FOR LEGISLATIVE SEARCH</note>
    <id>FIND_DATE_LEGISLATIVE</id>
    <sectionHeading>Find Legislative Dates</sectionHeading>
    <windowHeading>TRIPS - Search Legislative Dates</windowHeading>
    <showCreateNewButton>Y</showCreateNewButton>
    <criteriaFields>
      <field>
        <fieldName>TaxType</fieldName>
        <fieldType>string</fieldType>
        <fieldLabel>TaxType</fieldLabel>
        <columnName>REF_DATE_CFG.TAX_TYPE</columnName>
        <listId>REV_TYPE</listId>
      </field>
      <field>
        <fieldName>Description</fieldName>
        <fieldType>string</fieldType>
        <fieldLabel>Description</fieldLabel>
        <columnName>REF_DATE_CFG.DESCRIPTION</columnName>
      </field>
    </criteriaFields>
    <resultFields>
       <field>
        <columnOrderNbr>1</columnOrderNbr>
        <fieldName>Code</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>CODE</columnHeading>
        <columnName>REF_DATE_CFG.CODE</columnName>
        <isIdField>Y</isIdField>
        <isVisible>Y</isVisible>
      </field>
       <field>
        <columnOrderNbr>2</columnOrderNbr>
        <fieldName>Description</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>DESCRIPTION</columnHeading>
        <columnName>REF_DATE_CFG.DESCRIPTION</columnName>
       </field> 
       <field>
        <columnOrderNbr>3</columnOrderNbr>
        <fieldName>TaxType</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>TAXTYPE</columnHeading>
        <columnName>REF_DATE_CFG.TAX_TYPE</columnName>
      </field>
      <field>
        <columnOrderNbr>4</columnOrderNbr>
        <fieldName>FilingMode</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>FILINGMODE</columnHeading>
        <columnName>REF_DATE_CFG.FILING_MODE</columnName>
      </field>
      <field>
        <columnOrderNbr>5</columnOrderNbr>
        <fieldName>FilingFrequency</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>FILINGFREQUENCY</columnHeading>
        <columnName>REF_DATE_CFG.FILING_FREQUENCY</columnName>
      </field>
      <field>
        <columnOrderNbr>6</columnOrderNbr>
        <fieldName>RefDateRecurCfg</fieldName>
        <fieldType>string</fieldType>
        <columnHeading>REFDATERECURCFG</columnHeading>
        <columnName>REF_DATE_RECUR_CFG.DESCRIPTION</columnName>
      </field>
    </resultFields>
    <searchTables>
        <table>
            <name>REF_DATE_CFG</name>
        </table>
        <table>
            <name>REF_REV_TYPE</name>
        </table>
        <table>
            <name>REF_DATE_RECUR_CFG</name>
        </table>
    </searchTables>
    <joinQuery>REF_DATE_CFG.TAX_TYPE=REF_REV_TYPE.CODE and REF_DATE_CFG.REF_DATE_RECUR_CFG = REF_DATE_RECUR_CFG.CODE 
                and REF_DATE_CFG.REF_DATE_CATEGORY='LD'</joinQuery>
    <where>REF_DATE_CFG.REF_DATE_CATEGORY='LD'</where>
    <actions>
      <action>  
        <actionId>view</actionId>
        <actionLabel>View</actionLabel>
        <actionCallBack>view</actionCallBack>
        <allowableModes>viewMode</allowableModes>
      </action>
      <action>
        <actionId>edit</actionId>
        <actionLabel>Update</actionLabel>
        <actionCallBack>edit</actionCallBack>
        <allowableModes>editMode</allowableModes>
      </action>
      <action>
        <actionId>add</actionId>
        <actionLabel>Add</actionLabel>
        <actionCallBack>add</actionCallBack>
        <allowableModes>addMode</allowableModes>
      </action>
    </actions>
  </search>
</searchList>